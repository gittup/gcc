gtype_headers = gtype-c.h gt-c-parser.h gt-c-pragma.h gt-c-common.h gt-c-decl.h gt-tree-scalar-evolution.h gt-varasm.h gt-tree.h gt-stringpool.h gt-optabs.h gt-function.h gt-except.h gt-dwarf2out.h gt-dbxout.h gt-alias.h gt-integrate.h gt-i386.h gt-tree-ssa-structalias.h gt-ipa-reference.h gt-tree-phinodes.h gt-tree-ssa-propagate.h gt-cgraphunit.h gt-passes.h gt-targhooks.h gt-omp-low.h gt-tree-parloops.h gt-varpool.h gt-tree-nested.h gt-tree-profile.h gt-gimplify.h gt-tree-iterator.h gt-tree-ssa-address.h gt-tree-mudflap.h gt-gimple.h gt-stor-layout.h gt-sdbout.h gt-cfglayout.h gt-reginfo.h gt-lists.h gt-gcse.h gt-expr.h gt-explow.h gt-emit-rtl.h gt-dojump.h gt-tree-vect-generic.h gt-dwarf2asm.h gt-ipa-inline.h gt-cgraph.h gt-cselib.h gt-bitmap.h gt-caller-save.h gt-coverage.h
: gengtype-gen.o gengtype-lex-gen.o gengtype-parse-gen.o errors-gen.o $(BINUTILS_ROOT)/libiberty/libiberty.a |> !gen-ld |> gengtype
: gengtype | options.h |> ./%f . gtyp-input.list |> gtype-desc.c $(gtype_headers)
generated_headers += $(gtype_headers)

: options.c | $(generated_headers) |> ^ HOSTCC %f^ $(HOSTCC) -I. -I../include $(CFLAGS) -DGCC_DRIVER -c %f -o %o |> gcc-options.o {objs}
: foreach $(obj-y) | $(generated_headers) |> ^ HOSTCC %f^ $(HOSTCC) -I. -I../include $(CFLAGS) $(CFLAGS_%f) -c %f -o %o |> %B.o {objs}
: {objs} ../libcpp/libcpp.a $(BINUTILS_ROOT)/libiberty/libiberty.a ../libdecnumber/libdecnumber.a |> ^o LD %o^ $(HOSTCC) %f -o %o |> xgcc | $(GITTUP_ROOT)/<compiler_files>

: foreach $(backend-obj-y) | $(generated_headers) |> ^ HOSTCC %f^ $(HOSTCC) -I. -I../include -I../libcpp/include -I../libdecnumber -I../libdecnumber/bid $(CFLAGS) $(CFLAGS_%f) -Wno-variadic-macros -c %f -o %o |> %B.o {backend-objs}
: {backend-objs} intl.o opts-common.o version.o prefix.o |> ^ AR %o^ ar rcs %o %f |> libbackend.a

: foreach $(cc-obj-y) | $(generated_headers) |> ^ HOSTCC %f^ $(HOSTCC) -I. -I../include -I../libcpp/include -I../libdecnumber -I../libdecnumber/bid $(CFLAGS) $(CFLAGS_%f) -Wno-variadic-macros -c %f -o %o |> %B.o {cc-objs}
: {cc-objs} libbackend.a ../libcpp/libcpp.a ../libdecnumber/libdecnumber.a $(BINUTILS_ROOT)/libiberty/libiberty.a $(GITTUP_ROOT)/tup/libtup_client.a |> ^o LD %o^ $(HOSTCC) %f -o %o -lmpfr -lgmp -rdynamic -ldl |> cc1 | $(GITTUP_ROOT)/<compiler_files>

HOSTCC += -m32

compiler_proper = cc1 xgcc $(BINUTILS_ROOT)/gas/as
cc_proper = ./xgcc -B. -B$(BINUTILS_ROOT)/ld -B$(BINUTILS_ROOT)/gas -nostdinc -I./ginclude -isystem ../include-fixed -isystem $(UCLIBC_ROOT)/include/ -isystem $(UCLIBC_ROOT)/libpthread/linuxthreads.old/sysdeps/i386 -ftup-uclibc

crtflags += -include $(UCLIBC_ROOT)/include/libc-symbols.h
crtflags += -I$(LINUX_ROOT)/include
crtflags += -I$(LINUX_ROOT)/arch/x86/include

: foreach crtstuff.c | $(generated_headers) $(compiler_proper) |> ^o CC %f^ $(cc_proper) $(crtflags) -c %f -o %o -DIN_GCC -W -Wall -Wwrite-strings -Wcast-qual -Wstrict-prototypes -Wmissing-prototypes -Wold-style-definition -finhibit-size-directive -fno-inline -fno-exceptions -fno-zero-initialized-in-bss -fno-toplevel-reorder -fno-tree-vectorize -DENABLE_DECIMAL_BID_FORMAT -DCRT_BEGIN -I../include -I. |> crtbegin.o | $(GITTUP_ROOT)/<compiler_files>
: foreach crtstuff.c | $(generated_headers) $(compiler_proper) |> ^o CC %f^ $(cc_proper) $(crtflags) -c %f -o %o -DIN_GCC -W -Wall -Wwrite-strings -Wcast-qual -Wstrict-prototypes -Wmissing-prototypes -Wold-style-definition -finhibit-size-directive -fno-inline -fno-exceptions -fno-zero-initialized-in-bss -fno-toplevel-reorder -fno-tree-vectorize -DENABLE_DECIMAL_BID_FORMAT -DCRT_END -I../include -I. |> crtend.o | $(GITTUP_ROOT)/<compiler_files>
: foreach crtstuff.c | $(generated_headers) $(compiler_proper) |> ^o CC %f^ $(cc_proper) $(crtflags) -fPIC -c %f -o %o -DIN_GCC -W -Wall -Wwrite-strings -Wcast-qual -Wstrict-prototypes -Wmissing-prototypes -Wold-style-definition -finhibit-size-directive -fno-inline -fno-exceptions -fno-zero-initialized-in-bss -fno-toplevel-reorder -fno-tree-vectorize -DENABLE_DECIMAL_BID_FORMAT -DCRT_BEGIN -DCRTSTUFFS_O -I../include -I.  |> crtbeginS.o
: foreach crtstuff.c | $(generated_headers) $(compiler_proper) |> ^o CC %f^ $(cc_proper) $(crtflags) -fPIC -c %f -o %o -DIN_GCC -W -Wall -Wwrite-strings -Wcast-qual -Wstrict-prototypes -Wmissing-prototypes -Wold-style-definition -finhibit-size-directive -fno-inline -fno-exceptions -fno-zero-initialized-in-bss -fno-toplevel-reorder -fno-tree-vectorize -DENABLE_DECIMAL_BID_FORMAT -DCRT_END -DCRTSTUFFS_O -I../include -I.  |> crtendS.o

