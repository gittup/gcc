: foreach $(stupid-libgcc-y) | $(compiler_proper) $(generated_headers) |> ^ CC %f^ $(cc_proper) -O2 -DIN_GCC -W -Wall -Wwrite-strings -Wcast-qual -Wstrict-prototypes -Wmissing-prototypes -Wold-style-definition  -isystem ./include -fPIC -DHAVE_GTHR_DEFAULT -DIN_LIBGCC2 -D__GCC_FLOAT_NOT_NEEDED -DENABLE_DECIMAL_BID_FORMAT -DHAVE_CC_TLS -DUSE_TLS -fvisibility=hidden -DHIDE_EXPORTS -I. -I../include -I../libgcc -I$(LINUX_ROOT)/include -I$(LINUX_ROOT)/arch/x86/include -DL%B -include $(UCLIBC_ROOT)/include/libc-symbols.h -c %f -o %o |> %B.o {libgcc-objs}
: foreach $(libgcc-y) | $(compiler_proper) $(generated_headers) |> ^ CC %f^ $(cc_proper) -O2 -DIN_GCC -W -Wall -Wwrite-strings -Wcast-qual -Wstrict-prototypes -Wmissing-prototypes -Wold-style-definition  -isystem ./include -fPIC -DHAVE_GTHR_DEFAULT -DIN_LIBGCC2 -D__GCC_FLOAT_NOT_NEEDED -DENABLE_DECIMAL_BID_FORMAT -DHAVE_CC_TLS -DUSE_TLS -Wno-missing-prototypes -Wno-type-limits -fvisibility=hidden -DHIDE_EXPORTS -I. -I../include -I../libgcc -c %f -o %B.o |> %B.o {libgcc-objs}
: {libgcc-objs} |> ^o AR %o^ ar crs %o %f |> libgcc.a | $(GITTUP_ROOT)/<compiler_files>
