!gen-cc = | $(generated_headers) |> ^ HOSTCC %f^ $(HOSTCC) -c %f -o %o -I. -I../include -I../libcpp/include $(CFLAGS) -DGENERATOR_FILE |>
!gen-ld = |> ^ LD %o^ $(HOSTCC) %f -o %o |>
: foreach genmodes.c errors.c |> !gen-cc |> %B-gen.o

: genmodes-gen.o errors-gen.o $(BINUTILS_ROOT)/libiberty/libiberty.a |> !gen-ld |> genmodes
: genmodes |> ./%f -m > %o |> min-insn-modes.c
: genmodes |> ./%f > %o |> insn-modes.c
: genmodes |> ./%f -h > %o |> insn-modes.h
generated_headers += insn-modes.h

: foreach $(gen-y) |> !gen-cc |> %B-gen.o

